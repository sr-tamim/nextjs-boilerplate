{
  "name": "nextjs-boilerplate",
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "production": "next start -p 3000",
    "lint": "next lint",
    "lint:fix": "next lint --fix --ignore-path .gitignore .",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "security-check": "yarn audit --groups dependencies",
    "format": "prettier --write \"./**/*.{ts,tsx,js,jsx,css}\"",
    "format-check": "prettier --check \"./**/*.{ts,tsx,js,jsx,css}\"",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.6.1",
    "negotiator": "^1.0.0",
    "next": "15.5.3",
    "next-themes": "^0.4.6",
    "react": "19.1.1",
    "react-dom": "19.1.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "@types/negotiator": "^0.6.4",
    "@types/node": "^22.18.3",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "eslint": "^9.35.0",
    "eslint-config-next": "15.5.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2"
  },
  "packageManager": "yarn@4.9.4"
}
